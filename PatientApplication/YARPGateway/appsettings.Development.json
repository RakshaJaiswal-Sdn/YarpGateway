{
  "ReverseProxy": {
    "Routes": {
      "patient-service": {
        "ClusterId": "patientCluster",
        "Match": {
          "Path": "/api/Patient/{**catch-all}"
        }
      },
      "provider-service": {
        "ClusterId": "providerCluster",
        "Match": {
          "Path": "/api/Provider/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "patientCluster": {
        "Destinations": {
          "patient-service": {
            "Address": "https://localhost:7065/"
          }
        }
      },
      "providerCluster": {
        "Destinations": {
          "provider-service": {
            "Address": "https://localhost:7097/"
          }
        }
      }
    }
  }
}
